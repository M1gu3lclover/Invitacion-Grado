<svg width="800" height="600" xmlns="http://www.w3.org/2000/svg">
  <defs>
    <linearGradient id="bgGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1a1a2e;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#16213e;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#0f3460;stop-opacity:1" />
    </linearGradient>
    <radialGradient id="starGlow">
      <stop offset="0%" style="stop-color:#d4af37;stop-opacity:0.8" />
      <stop offset="100%" style="stop-color:#d4af37;stop-opacity:0" />
    </radialGradient>
  </defs>
  
  <!-- Background -->
  <rect width="800" height="600" fill="url(#bgGradient)"/>
  
  <!-- Stars -->
  <circle cx="100" cy="80" r="2" fill="#d4af37" opacity="0.8"/>
  <circle cx="250" cy="120" r="1.5" fill="#f4e4c1" opacity="0.6"/>
  <circle cx="600" cy="100" r="2" fill="#d4af37" opacity="0.7"/>
  <circle cx="700" cy="200" r="1" fill="#f4e4c1" opacity="0.5"/>
  <circle cx="150" cy="300" r="1.5" fill="#d4af37" opacity="0.6"/>
  <circle cx="500" cy="400" r="2" fill="#f4e4c1" opacity="0.8"/>
  <circle cx="300" cy="500" r="1" fill="#d4af37" opacity="0.5"/>
  <circle cx="650" cy="450" r="1.5" fill="#f4e4c1" opacity="0.7"/>
  
  <!-- Glowing stars -->
  <circle cx="400" cy="150" r="30" fill="url(#starGlow)" opacity="0.3"/>
  <circle cx="200" cy="400" r="40" fill="url(#starGlow)" opacity="0.2"/>
  <circle cx="600" cy="350" r="35" fill="url(#starGlow)" opacity="0.25"/>
  
  <!-- Graduation cap silhouette -->
  <g transform="translate(400, 300)" opacity="0.15">
    <!-- Cap top -->
    <rect x="-60" y="-20" width="120" height="8" fill="#d4af37" rx="2"/>
    <!-- Cap base -->
    <path d="M -40,-20 L -40,0 L 40,0 L 40,-20 Z" fill="#d4af37"/>
    <!-- Tassel -->
    <line x1="60" y1="-16" x2="75" y2="10" stroke="#d4af37" stroke-width="2"/>
    <circle cx="75" cy="10" r="4" fill="#d4af37"/>
  </g>
  
  <!-- Sparkles -->
  <g opacity="0.6">
    <path d="M 120,200 L 122,205 L 127,207 L 122,209 L 120,214 L 118,209 L 113,207 L 118,205 Z" fill="#f4e4c1"/>
    <path d="M 680,150 L 682,155 L 687,157 L 682,159 L 680,164 L 678,159 L 673,157 L 678,155 Z" fill="#d4af37"/>
    <path d="M 350,450 L 352,455 L 357,457 L 352,459 L 350,464 L 348,459 L 343,457 L 348,455 Z" fill="#f4e4c1"/>
    <path d="M 550,250 L 552,255 L 557,257 L 552,259 L 550,264 L 548,259 L 543,257 L 548,255 Z" fill="#d4af37"/>
  </g>
</svg>
